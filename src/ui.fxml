<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.GridPane?>
<GridPane fx:controller = "GameCode"
          stylesheets="/game.css"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Label id = "scores" fx:id = "scores" stylesheets = "game.css"/>
    <Label id = "label" fx:id = "label" stylesheets = "game.css"/>
    <Button id = "button" fx:id = "button" stylesheets = "game.css" alignment = "center"/>
</GridPane>